[diff]
  external = $HOME/home-bin/git_diff_wrapper

[pager]
  diff =
[user]
	name = Firstname Lastname
	email = firstname@lastname.de
[alias]
	lol = log --graph --decorate --pretty=oneline --abbrev-commit --all
	lg = log -p
	st = status
	ci = commit
	br = branch
	co = checkout
	df = diff
	lg = log -p
[push]
	default = current
[color]
	ui = true
